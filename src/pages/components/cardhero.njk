{# Sets the layout to be extended #}
{% extends "layouts/component.njk" %}

{# Sets page-specific variables #}
{% set page = {
  "data": page_component,
  "title": "Card hero",
  "noSidePad" : true
} %}

{# Imports components #}
{% import 'components/cardhero/cardhero.njk' as CardHero %}

{% block head_css %}
  <link rel="stylesheet" href="../assets/css/general.bundle.css">
{% endblock %}

{% block content %}
  <div class="zaux-theme--light1">
    {{ CardHero.content(
      {
        "eyelet" : "Eyelet here"
      }
    ) }}
  </div>
{% endblock %}

{% block foot_js %}
  <script defer src="../assets/js/general.bundle.js"></script>
{% endblock %}
