                                            }
                                        },
                                        {
                                            "mainLevel" : {
                                                "label" : "First level label",
                                                "link" : "#first-level-link"
                                            },
                                            "subLevel" : [
                                                {
                                                    "mainLevel" : {
                                                        "highlight" : true,
                                                        "link" : "#test-highlight",
                                                        "label" : "Sublevel title linked highlighted"
                                                    }
                                                },
                                                {
