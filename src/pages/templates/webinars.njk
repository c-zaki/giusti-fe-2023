{% block head_css %}
  <link rel="stylesheet" href="../assets/css/general.bundle.css">
{% endblock %}

{% block content %}

    <!-- Hero Banner -->
        {{ BannerImg1.content() }}
    <!-- End of hero banner -->

    <!-- Breadcrumbs -->
        <div class="container-l pb-6 pt-7 pt-md-9 pb-md-7">
            {{ Breadcrumbs.content() }}
        </div>
    <!-- End of breadcrumbs -->

    <!-- Content layout -->

        {# START OF CONTENT BUFFER #}

            {# Content #}
              {% set titleBannerContent %}
                  <!-- Title banner -->
                      {{
                          TitleBanner.content(
                              {
                                  "contained" : false,
                                  "title" : "Webinar",
                                  "subtitle" : "Per sapere di più su Nikè iscriviti e partecipa ai nostri webinar o guarda quelli passati."
                              }
                          )
                      }}
                  <!-- End of title banner -->
              {% endset %}
              {% set mediaCardContent %}
                  {{ MediaCard1.content() }}
              {% endset %}
              {% set mediaCardContentNoFbox %}
                  {{ MediaCard1.content({"title" : "Next webinar", "noFancyBox" : true, "url" : "#no-fancybox-link" }) }}
              {% endset %}
              {% set gridVids1 %}
                {{
                    GridList1.content(
                        {
                          "cols" : "col-12 col-sm-6",
                          "contained" : false,
                          "elements" : [
                              { "content" : mediaCardContent },
                              { "content" : mediaCardContentNoFbox }
                          ]
                        }
                    )
                }}
              {% endset %}
              {% set gridVids2 %}
                {% set richTextSample %}
                    <h3>Archivio</h3>
                {% endset %}
                {{ RichTextBlock1.content({"content" : richTextSample }) }}
                {{
                    GridList1.content(
                        { 
                          "cols" : "col-12 col-sm-6 col-md-3",
                          "contained" : false,
                          "elements" : [
                            { "content" : mediaCardContent },
                            { "content" : mediaCardContent },
                            { "content" : mediaCardContent },
                            { "content" : mediaCardContent },
                            { "content" : mediaCardContent }
                          ]
                        }
                    )
                }}
              {% endset %}
            {# End of content #}

            {# Sidebar #}
                {% set SummaryContent %}
                    <!-- Summary -->
                        {{
                            TreeNav.content(
                                {
                                    "title" : "Tree nav title",
                                    "backBtn" : {
                                        "link" : "#back-link",
                                        "label" : "Scheda Principale"
                                    },
                                    "entries" : [
                                        {
                                            "mainLevel" : {
                                                "label" : "First level label",
