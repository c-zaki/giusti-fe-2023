        {{ BannerImg2.content( { "img" : { "mainSrc" : zaux_config.properties.mediaDistURL + "/img/nike-magazine-large.svg" } } ) }}
    <!-- End of hero banner -->

    <!-- Breadcrumbs -->
        <div class="container-l py-6">
            {{ Breadcrumbs.content() }}
