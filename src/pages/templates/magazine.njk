        {% set cardSample2 %}
        {{
          CardWide1.content(
            {
              "className" : ["h-100"],
              "title" : "Title 2"
            }
          )
        }}
        {% endset %}

        {{ SlideShow2.content(
            {
              "className" : ["mb-6"],
                "activateControls" : true,
                "slides" : [
                    cardSample,
                    cardSample2,
                    cardSample
                ]
            }
        ) }}

